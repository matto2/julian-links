---
import Layout from "../layouts/Layout.astro";
import { Image } from "astro:assets";
import BioImage from "../assets/images/julian-1.png";
import Card from "../components/Card.astro";
import LittleCard from "../components/LittleCard.astro";
import Insta from "../components/Insta.astro";
import Button from "../components/Button.astro";
import "@fontsource/dela-gothic-one";
import { AstroSeo } from "@astrolib/seo";
import JulesFace from "../components/JulesFace.jsx";
import RocketLaunch from "astro-heroicons/solid/RocketLaunch.astro";
import Icons from "../components/Icons.astro";
import Footer from "../components/Footer.astro";
// Supports weights 100-900
import "@fontsource-variable/montserrat";
---

<AstroSeo
  title="Julian O’Leary Links"
  description="A collection of links for Julian O'Leary also known as Julian T O'Leary"
/>

<Layout>
  <!-- <p class="w-8 text-slate-100">
    <RocketLaunch />
  </p> -->

  <div class="flex-grow">
    <div
      class="text-4xl font-variable font-medium my-6 text-red-600 pt-12 text-center"
    >
      Julian O’Leary
    </div>

    <JulesFace client:load />

    <div class="mx-auto w-72">
      <Icons />
    </div>
  </div>

  <Footer />
</Layout>
